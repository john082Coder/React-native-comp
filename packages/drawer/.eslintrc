{
  "extends": "eslint-config-satya164",

  "plugins": ["react-native-globals"],

  "env": {
    "es6": true,
    "react-native-globals/all": true
  },

  "settings": {
    "import/core-modules": ["react-native-screens"]
  },

  "rules": {
    "import/named": "off",
    "import/default": "off",
    "import/namespace": "off",
    "react-native/no-inline-styles": "off"
  }
}
