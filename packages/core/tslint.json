{
  "extends": "tslint:recommended",
  "rules": {
    "arrow-parens": [true, "ban-single-arg-parens"],
    "arrow-return-shorthand": [true, "multiline"],
    "trailing-comma": false,
    "quotemark": {
      "options": [
        "single",
        "avoid-escape"
      ]
    },
    "object-literal-sort-keys": false
  }
}
